    <idno type="siddham_object_identifier">#SO:Object ID#</idno>
    <msName>#SO:Subtitle#</msName>
  </msIdentifier>
  <physDesc>
      <objectDesc>
          <supportDesc>
            <support>
              <material>#SO:Description/material#</material>
              <objectType>#SO:Description/object type#</objectType>
