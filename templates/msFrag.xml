          </supportDesc>
      </objectDesc>
  </physDesc>
  <history>
    <origin>
      <origPlace>#SO:History/Created/Place#</origPlace>
      <origDate>#SO:History/Created/Date#</origDate>
    </origin>
    <provenance type="found">#SO:History/Found/Date#</provenance>
